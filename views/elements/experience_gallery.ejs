<div class="experience_gallery">
    <% for(let i = 1; i < 7; i++) { %>
        <figure class="revealFadeIn gallery__item gallery__item--<%=i.toString()%>">
            <img src="images/experience/<%=i.toString()%>.jpg" class="gallery__img" width="100" height="200" alt="Image">
        </figure>
    <% } %>
</div>